{
  "name": "@tasky/api-client",
  "version": "1.0.1",
  "main": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.js"
    }
  },
  "files": [
    "index.js",
    "index.d.ts",
    "core",
    "models",
    "services"
  ],
  "scripts": {
    "generate": "./node_modules/.bin/openapi-ts -i ./openapi.json -o src/api-client -c @hey-api/client-axios",
    "build": "tsc -p tsconfig.api.json",
    "prepare": "npm version patch && npm run generate && npm run build"
  },
  "devDependencies": {
    "openapi-typescript-codegen": "^0.25.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@hey-api/client-axios": "^0.6.2",
    "axios": "^1.6.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
